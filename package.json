{
  "name": "nestjs-graphql-prisma-boilerplate",
  "version": "0.0.1",
  "description": "",
  "author": "",
  "private": true,
  "license": "UNLICENSED",
  "scripts": {
    "build": "nest build",
    "format": "prettier --write \"src/**/*.ts\" \"test/**/*.ts\"",
    "start": "cross-env nest start",
    "start:dev": "cross-env nest start --watch",
    "start:debug": "cross-env nest start --debug --watch",
    "start:prod": "cross-env node dist/main",
    "start:docker": "cross-env node dist/main",
    "lint": "eslint \"{src,apps,libs,test}/**/*.ts\" --fix",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:cov": "jest --coverage",
    "test:debug": "node --inspect-brk -r tsconfig-paths/register -r ts-node/register node_modules/.bin/jest --runInBand",
    "test:e2e": "jest --config ./test/jest-e2e.json",
    "dotenv:dev": "dotenv -e .env.development",
    "dotenv:prod": "dotenv -e .env.production",
    "dotenv:docker": "dotenv -e .env.docker",
    "set-version-openapi-generator-cli": "openapi-generator-cli version-manager set 6.2.0",
    "openapi:generator": "openapi-generator-cli generate -g typescript-axios -i swagger.yml -o ./openapi --additional-properties npmVersion=%npm_package_version%,npmName=%npm_package_name%-openapi,supportsES6=true,withInterfaces=true --git-repo-id=%npm_package_name%-openapi --git-user-id=ume-software",
    "openapi:publish": "cd openapi && npm publish && cd ../",
    "openapi": "npm run openapi:generator && npm run openapi:publish"
  },
  "dependencies": {
    "@nestjs/apollo": "^12.1.0",
    "@nestjs/cache-manager": "^2.2.1",
    "@nestjs/common": "^10.1.3",
    "@nestjs/config": "^3.0.0",
    "@nestjs/core": "^10.1.3",
    "@nestjs/graphql": "^12.0.8",
    "@nestjs/jwt": "^10.1.0",
    "@nestjs/mongoose": "^10.0.4",
    "@nestjs/platform-express": "^10.1.3",
    "@nestjs/platform-socket.io": "^10.1.3",
    "@nestjs/schedule": "^4.0.2",
    "@nestjs/serve-static": "^4.0.0",
    "@nestjs/swagger": "^7.1.8",
    "@nestjs/websockets": "^10.1.3",
    "@types/cron": "^2.4.0",
    "apollo-server-express": "^3.12.0",
    "axios": "^1.4.0",
    "bcrypt": "^5.1.0",
    "cache-manager": "^5.4.0",
    "class-transformer": "^0.5.1",
    "class-validator": "^0.14.0",
    "firebase-admin": "^12.0.0",
    "google-auth-library": "^9.0.0",
    "googleapis": "^133.0.0",
    "graphql": "^16.8.0",
    "graphql-tools": "^9.0.0",
    "js-yaml": "^4.1.0",
    "jsonwebtoken": "^9.0.1",
    "lodash": "^4.17.21",
    "moment": "^2.29.4",
    "mongodb": "^6.4.0",
    "mongoose": "^8.2.1",
    "passport-jwt": "^4.0.1",
    "reflect-metadata": "^0.2.1",
    "rxjs": "^7.8.1"
  },
  "devDependencies": {
    "@nestjs/cli": "^10.1.11",
    "@nestjs/schematics": "^10.0.2",
    "@nestjs/testing": "^10.1.3",
    "@openapitools/openapi-generator-cli": "^2.7.0",
    "@types/bcrypt": "^5.0.0",
    "@types/dotenv": "^8.2.0",
    "@types/express": "^4.17.17",
    "@types/jest": "^29.5.3",
    "@types/js-yaml": "^4.0.5",
    "@types/jsonwebtoken": "^9.0.2",
    "@types/lodash": "^4.14.197",
    "@types/node": "^20.5.0",
    "@types/passport-jwt": "^4.0.1",
    "@types/supertest": "^6.0.2",
    "@typescript-eslint/eslint-plugin": "^7.1.1",
    "@typescript-eslint/parser": "^7.1.1",
    "cross-env": "^7.0.3",
    "dotenv": "^16.3.1",
    "dotenv-cli": "^7.2.1",
    "eslint": "^8.47.0",
    "eslint-config-prettier": "^9.0.0",
    "eslint-plugin-prettier": "^5.0.0",
    "jest": "^29.6.2",
    "prettier": "^3.0.1",
    "source-map-support": "^0.5.21",
    "supertest": "^6.3.3",
    "ts-jest": "^29.1.1",
    "ts-loader": "^9.4.4",
    "ts-node": "^10.9.1",
    "tsconfig-paths": "^4.2.0",
    "typescript": "^5.1.6"
  },
  "jest": {
    "moduleFileExtensions": [
      "js",
      "json",
      "ts"
    ],
    "rootDir": "src",
    "modulePaths": [
      "<rootDir>"
    ],
    "testRegex": ".*\\.spec\\.ts$",
    "transform": {
      "^.+\\.(t|j)s$": "ts-jest"
    },
    "collectCoverageFrom": [
      "**/*.(t|j)s"
    ],
    "coverageDirectory": "../coverage",
    "testEnvironment": "node",
    "moduleNameMapper": {
      "^src/(.*)$": "<rootDir>/$1"
    }
  }
}
